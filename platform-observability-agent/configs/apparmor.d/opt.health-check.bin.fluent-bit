  /usr/libexec/sudo/sudoers.so rm,
  /usr/local/bin/yq ix,
  /usr/local/bin/yq_linux_amd64 ix,
  /usr/local/lib/** r,
  /usr/sbin/ r,
  /usr/sbin/aa-status ix,
