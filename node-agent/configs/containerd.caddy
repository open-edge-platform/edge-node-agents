localhost.internal:9443 {
    bind 127.0.0.1
    tls /etc/intel_edge_node/caddy/certs/caddy-containerd-cert.pem /etc/intel_edge_node/caddy/.keys/caddy-containerd-key.pem {
        protocols tls1.3
        ciphers TLS_AES_256_GCM_SHA384
    }
    reverse_proxy https://registry-rs.internal.example.intel.com {
        header_up Authorization "Bearer RS_AT"
        header_up Host {upstream_hostport}
     }
    log {
        level DEBUG
        format filter {
            wrap console
            fields {
                resp_headers delete
                request>headers delete
                request>tls delete
            }
        }
    }
}
